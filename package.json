{
  "name": "vue-echarts",
  "version": "6.0.2",
  "description": "Vue.js component for Apache ECharts.",
  "author": "GU Yiling <justice360@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run docs && rimraf dist && rollup -c rollup.config.js && cp src/index.vue2.d.ts dist",
    "lint": "vue-cli-service lint",
    "build:demo": "vue-cli-service build",
    "docs": "node -r esm ./scripts/docs.js",
    "postinstall": "node ./scripts/postinstall.js",
    "prepare": "npm run build"
  },
  "main": "dist/index.cjs.min.js",
  "module": "dist/index.esm.min.js",
  "unpkg": "dist/index.umd.min.js",
  "files": [
    "dist",
    "scripts/postinstall.js"
  ],
  "dependencies": {
    "resize-detector": "^0.3.0",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "^5.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^5.0.0-beta.1",
    "@vue/cli-plugin-typescript": "^5.0.0-beta.1",
    "@vue/cli-service": "^5.0.0-beta.1",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/composition-api": "^1.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "codesandbox": "^2.2.3",
    "comment-mark": "^1.0.0",
    "echarts": "^5.2.2",
    "eslint": "^7.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "esm": "^3.2.25",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.0.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    "resize-detector": "^0.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "~4.1.5",
    "vue": "^3.2.26"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.5",
    "echarts": "^5.1.2",
    "vue": "^2.6.12 || ^3.1.1"
  },
  "jsdelivr": "dist/index.umd.min.js",
  "license": "MIT",
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "repository": "https://github.com/ecomfe/vue-echarts.git",
  "types": "dist/index.d.ts"
}
