{
  "include": [
    "vite.config.*",
    "vitest.config.*",
    "eslint.config.*",
    "rollup.config.*"
  ],
  "compilerOptions": {
    "composite": true,
    "lib": ["ESNext"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",

    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"]
  }
}
